TOPDIR = @top_srcdir@
ifndef MAKDIR
MAKDIR = @top_srcdir@
endif
srcdir = @srcdir@
VPATH  = @srcdir@
TOPOBJ = @TOPOBJ@

include $(MAKDIR)/Makeconf

# windos 95 does not build this
ifneq ($(SHORTSYS),djgpp)
EXECUTABLES = auth2 initpw au oldtokens
SSBINS      = auth2 initpw 
CSBINS      = au
endif

LIBRARIES   = libauth2.a
HEADERS     = ${RP2FILES} ${RP2HEADERS}
OBJS = auth2.client.o auth2.server.o auth2.multi.o au.o auser.o authmon.o avenus.o avice.o initpw.o oldtokens.o 


RP2FILES    = auth2.rpc2
RP2HEADERS  = $(notdir ${RP2FILES:.rpc2=.h})

LIBS=	${LIBRPC2}\
	${LIBAL}\
	${LIBOLDLWP}\
	${LIBUTIL} \
	${LIBKERNDEP} \
	${LIBCOMPAT}

libauth2.a: auth2.server.o auth2.client.o avice.o auser.o avenus.o 

auth2: auth2.o libauth2.a $(LIBS)
	$(CXX) $(LIBFLAGS) auth2.o libauth2.a $(LIBS) -o auth2

oldtokens: oldtokens.o libauth2.a $(LIBS)
	$(CXX) $(LIBFLAGS) oldtokens.o libauth2.a $(LIBS) -o oldtokens

initpw: initpw.o $(LIBS)
	$(CXX) $(LIBFLAGS) initpw.o $(LIBS) -o initpw

au: au.o libauth2.a $(LIBS)
	$(CXX) $(LIBFLAGS) au.o libauth2.a $(LIBS) -o au


include $(MAKDIR)/configs/Makerules
