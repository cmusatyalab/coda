TOPDIR = @top_srcdir@
ifndef MAKDIR
MAKDIR = @top_srcdir@
endif
srcdir = @srcdir@
VPATH  = @srcdir@
TOPOBJ = @TOPOBJ@

include $(MAKDIR)/Makeconf

ifneq ($(SHORTSYS),djgpp)
EXECUTABLES = auth2 initpw au clog cpasswd ctokens cunlog
CBINS       = clog cpasswd ctokens cunlog
CSBINS      = au
SBINS       = au
SSBINS      = auth2 initpw
endif


RP2FILES    = auth2.rpc2
RP2HEADERS  = $(notdir ${RP2FILES:.rpc2=.h})

LIBS=	${LIBRPC2}\
	${LIBAL}\
	${LIBOLDLWP}\
	${LIBUTIL} \
	${LIBKERNDEP} \
	${LIBAUSER}\
	${LIBCOMPAT}

CFLAGS := $(CFLAGS) $(KRBINCLFLAGS)

LIBRARIES   = libauth2.a libauser.a
HEADERS     = ${RP2FILES} ${RP2HEADERS}
OBJS = auth2.client.o auth2.server.o auth2.multi.o au.o auser.o authmon.o avenus.o avice.o initpw.o

ifeq ($(KERBEROS),)
LIBAUTH2OBJS=auth2.server.o acommon.o avice.o pwsupport.o
LIBAUSEROBJS=auth2.client.o acommon.o auser.o avenus.o
else
LIBAUTH2OBJS=auth2.server.o acommon.o avice.o krbsupport.o pwsupport.o
LIBAUSEROBJS=auth2.client.o acommon.o auser.o avenus.o krbsupport.o
endif

libauth2.a: $(LIBAUTH2OBJS)

libauser.a: $(LIBAUSEROBJS)

auth2: auth2.o libauth2.a $(LIBS)
	$(CC) $(LIBFLAGS) auth2.o libauth2.a $(LIBS) $(LIBKRBS) -o auth2


initpw: initpw.o $(LIBS)
	$(CC) $(LIBFLAGS) initpw.o $(LIBS) -o initpw

au: au.o libauser.a $(LIBS)
	$(CC) $(LIBFLAGS) au.o libauser.a $(LIBS) $(LIBKRBS) -o au

clog: clog.o libauser.a ${LIBS}
	${CC} $(LIBFLAGS) clog.o libauser.a ${LIBS} $(LIBKRBS) -o clog

cunlog: cunlog.o libauser.a ${LIBS}
	${CC} $(LIBFLAGS) cunlog.o libauser.a ${LIBS} -o cunlog

cpasswd: cpasswd.o libauser.a ${LIBS}
	${CC} $(LIBFLAGS) cpasswd.o  libauser.a ${LIBS} ${LIBKRBS} -o cpasswd

ctokens: ctokens.o libauser.a ${LIBS}
	${CC} $(LIBFLAGS) ctokens.o libauser.a ${LIBS} -o ctokens



include $(MAKDIR)/configs/Makerules
