TOPDIR = @top_srcdir@
ifndef MAKDIR
MAKDIR = @top_srcdir@
endif
srcdir = @srcdir@
VPATH  = @srcdir@
TOPOBJ = @TOPOBJ@

include $(MAKDIR)/Makeconf

EXECUTABLES = filerepair removeinc
SCRIPTS     = xfrepair xaskuser
CBINS       = $(EXECUTABLES) $(SCRIPTS)

LIBS=	$(LIBKERNDEP)

removeinc: removeinc.o $(LIBS)
	$(CC) $(LIBFLAGS) $^ ${LIBS} -o removeinc

filerepair: filerepair.o $(LIBS)
	$(CC) $(LIBFLAGS) $^ ${LIBS} -o filerepair

include $(MAKDIR)/configs/Makerules
