TOPDIR = @top_srcdir@
ifndef MAKDIR
MAKDIR = @top_srcdir@
endif
srcdir = @srcdir@
VPATH  = @srcdir@
TOPOBJ = @TOPOBJ@

include $(MAKDIR)/Makeconf

ifneq ($(SHORTSYS),djgpp)
EXECUTABLES = clog cpasswd ctokens cunlog testlog
CBINS       = clog cpasswd ctokens cunlog
TESTS       = testlog
endif

LIBS=	${LIBAUTH2}	\
	${LIBRPC2}	\
	${LIBOLDLWP} 	\
	${LIBKERNDEP}	\
	$(LIBCOMPAT)

clog: clog.o ${LIBS}
	${CXX} $(LIBFLAGS) clog.o ${LIBS} -o clog

cunlog: cunlog.o ${LIBS}
	${CXX} $(LIBFLAGS) cunlog.o ${LIBS} -o cunlog

cpasswd: cpasswd.o ${LIBS}
	${CXX} $(LIBFLAGS) cpasswd.o ${LIBS} -o cpasswd

ctokens: ctokens.o ${LIBS}
	${CXX} $(LIBFLAGS) ctokens.o ${LIBS} -o ctokens

testlog: testlog.o ${LIBS}
	${CXX} $(LIBFLAGS) testlog.o ${LIBS} -o testlog

# Dependencies
clog.o:		clog.cc ${INCLDIR}/auth2.h ${INCLSPECIAL}/lwp.h ${INCLDIR}/rpc2.h
cunlog.o:	cunlog.cc ${INCLDIR}/auth2.h
cpasswd.o:	cpasswd.cc ${INCLDIR}/auth2.h ${INCLDIR}/rpc2.h 
ctokens.o:	ctokens.cc ${INCLDIR}/auth2.h ${INCLDIR}/rpc2.h 
testlog.o:	testlog.cc ${INCLDIR}/auth2.h ${INCLDIR}/rpc2.h

include $(MAKDIR)/configs/Makerules
