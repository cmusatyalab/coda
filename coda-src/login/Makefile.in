TOPDIR= @top_srcdir@
srcdir = @srcdir@
VPATH = @srcdir@
TOPOBJ= @TOPOBJ@

include $(TOPDIR)/Makeconf

LIBS=	${LIBAUTH2}	\
	${LIBRPC2}	\
	${LIBOLDLWP} 	\
	${LIBCODASYS}	\
	$(LIBFLAGS)

LIBS_pmax_mach = 
LIBS_i386_mach = 
LIBS_i386_nbsd1 = -lcompat 
LIBS_sun4_mach = 

# Identify files of interest

EXECUTABLES= clog cpasswd ctokens cunlog testlog
BINS=clog cpasswd ctokens cunlog
TESTS=testlog
CFILES= clog.c cpasswd.c ctokens.c cunlog.c testlog.c

# top-level options
clean:	
	rm -f *.o clog cpasswd ctokens cunlog testlog

# programs

clog: clog.o
	${CXX} -o clog clog.o ${LIBS} $(LIBS_${SYS}) ${CXXFLAGS}

cunlog: cunlog.o
	${CXX} -o cunlog cunlog.o ${LIBS} $(LIBS_${SYS}) ${CXXFLAGS}

cpasswd: cpasswd.o
	${CXX} -o cpasswd cpasswd.o ${LIBS} $(LIBS_${SYS}) ${CXXFLAGS}

ctokens: ctokens.o
	${CXX} -o ctokens ctokens.o ${LIBS} $(LIBS_${SYS}) ${CXXFLAGS}

testlog: testlog.o
	${CXX} -o testlog testlog.o ${LIBS}  $(LIBS_${SYS}) ${CXXFLAGS}

# objects
clog.o:		clog.cc ${INCLDIR}/auth2.h ${INCLSPECIAL}/lwp.h ${INCLDIR}/rpc2.h
cunlog.o:	cunlog.cc ${INCLDIR}/auth2.h
cpasswd.o:	cpasswd.cc ${INCLDIR}/auth2.h ${INCLDIR}/rpc2.h 
ctokens.o:	ctokens.cc ${INCLDIR}/auth2.h ${INCLDIR}/rpc2.h 
testlog.o:	testlog.cc ${INCLDIR}/auth2.h ${INCLDIR}/rpc2.h

include $(TOPDIR)/Makerules
