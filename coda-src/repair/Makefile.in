TOPDIR= @top_srcdir@
srcdir = @srcdir@
VPATH = @srcdir@
TOPOBJ= @TOPOBJ@

include $(TOPDIR)/Makeconf


LIBS = ${LIBRESOLVE} ${LIBVV} ${LIBAUTH2} ${LIBCODASYS}

# Files of interest
HFILES = repair.h 
CFILES = reptest.cc repair.cc rvol.cc path.cc
OFILES = rvol.o path.o
# TESTS = reptest
EXECUTABLES = repair

reptest: reptest.o rvol.o path.o 
	$(CXX) $(CXXFLAGS) $(OFILES) reptest.o $(LIBFLAGS) $(LIBS) -o reptest

repair:   repair.o rvol.o path.o 
	$(CXX) $(CXXFLAGS) $(OFILES) repair.o $(LIBFLAGS) $(LIBS) $(LIBCS) -o repair

include $(TOPDIR)/Makerules

