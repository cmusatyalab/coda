#! /bin/sh

set -e

case "$1" in
    install)
        dpkg-divert --package coda-client --rename --divert /usr/sbin/codaconfedit.coda-server --add /usr/sbin/codaconfedit
    ;;

    upgrade|abort-upgrade)
	umount /coda >/dev/null 2>&1 || true
    ;;

    *)
        echo "preinst called with unknown argument \`$1'" >&2
        exit 0
    ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0


